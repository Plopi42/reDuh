<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_jar" name="Create reDuhClient JAR">
    <target name="create_jar">
        <jar destfile="dist/reDuhClient.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="com.sensepost.reDuh.reDuhClient"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="bin"/>
        </jar>
    </target>
</project>
